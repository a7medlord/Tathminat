@using CloudApp.Models.BusinessModel
@model IEnumerable<CloudApp.Models.BusinessModel.ApplicationUser>

@{
    ViewData["Title"] = "الرئيســة";
}
<h3>المستخدمين والصلاحيات</h3>
<p>
    <a asp-action="Register" class="btn btn-primary">اضافه جديد</a>
</p>
<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-primary">
            <div class="panel-heading clearfix">
                <h3 class="panel-title"> اداره المستخدمين</h3>
                <ul class="panel-tool-options">
                    <li><a data-rel="collapse" href="#"><i class="icon-down-open"></i></a></li>
                    <li><a data-rel="reload" href="#"><i class="icon-arrows-ccw"></i></a></li>
                    <li><a data-rel="close" href="#"><i class="icon-cancel"></i></a></li>
                </ul>
            </div>
            <div class="panel-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered table-hover dataTables-example">
                        <thead>
                            <tr>
                                <th>
                                  اسم الموظف
                                </th>
                                <th>
                                    الايميل
                                </th>
                                <th>
                                   رقم الهاتف
                                </th>
                                <th>
                                    رقم العضوية
                                </th>
                                <th>
                                    نسبة الادخال
                                </th>
                                <th>
                                    نسبة التثمين
                                </th>
                                <th>
                                    نسبة التدقيق
                                </th>
                                <th>
                                    نسبة الاعتماد
                                </th>
                                <th>
                                    نسبة الاشراف
                                </th>
                                <th>
                                   رقم الهوية
                                </th>
                                <th>
                                   اسم المستخدم
                                </th>
                               
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.EmployName)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.Email)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.PhoneNumber)
                                    </td>
                               
                                    <td>
                                        @Html.DisplayFor(modelItem => item.MemberId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.InterPercentage)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.MuthminPercentage)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.AduitPercentage)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.AproverPercentage)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.SupervisionPercentage)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.IdentityId)
                                    </td>
                                    <td>
                                        @Html.DisplayFor(modelItem => item.UserName)
                                    </td>
                                    
                                    <td>
                                        <a asp-action="EditRegister" asp-route-id="@item.Id" class="text-primary"><i class="glyphicon glyphicon-edit"></i></a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
